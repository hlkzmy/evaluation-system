<div class="row-fluid"><!-- BEGIN PAGE HEADER-->
	<div class="span12">
        <ul class="breadcrumb"><!-- BEGIN PAGE TITLE & BREADCRUMB-->
               <li>
                  <a href="#">网站热力图</a>
               </li>
               <li>
                   <a href="#">页面管理</a>
               </li>
               <li>
                   <a href="#">页面列表</a>
               </li>
        </ul><!-- END PAGE TITLE & BREADCRUMB-->
    </div><!-- END span12-->
</div><!-- END PAGE HEADER-->
<div class="row-fluid"><!-- BEGIN PAGE CONTENT-->
	<div class="col-md-12">
		<table class="table table-striped table-bordered table-hover datatable">
		     <thead>
		        <tr>
		            <th>编号</th>
		            <th>点击图名称</th>
		            <th>页面url</th>
		            <th>创建日期</th>
		            <th>操作</th>
		        </tr>
		     </thead>
		     <tbody id="data_table_1_tbody">
		     	{%for item in page%}
					<tr>
		                <td>{{item.id}}</td>
		                <td>{{item.name}}</td>
		                <td>{{item.url}}</td>
		                <td>{{item.createTime|date('Y-m-d H:i:s')}}</td>
		                <td>
		                    <a type="button" class="btn red btn-xs ajaxify"  href="{{ path('common_page_view',{'id':item.id})}}">
		                                                查看热点图
		                    </a>
		                    <a type="button" class="btn green btn-xs"  data-toggle="modal" href="#large">
		                    	查看JS代码
		                    </a>
		                    <a type="button" class="btn red btn-xs" href="{{ path('common_page_view',{'id':item.id})}}">
		                       	删除
		                    </a>
		                </td>
		            </tr>
				{%endfor%}
		     </tbody>
		</table>
	</div>
</div><!-- END PAGE CONTENT-->
