<div class="row-fluid"><!-- BEGIN PAGE HEADER-->
	<div class="span12">
        <ul class="breadcrumb"><!-- BEGIN PAGE TITLE & BREADCRUMB-->
               <li>
                  <a href="#">民主评价系统</a>
               </li>
               <li>
                   <a href="#">测评单位管理</a>
               </li>
               <li>
                   <a href="#">单位列表</a>
               </li>
        </ul><!-- END PAGE TITLE & BREADCRUMB-->
    </div><!-- END span12-->
</div><!-- END PAGE HEADER-->
<div class="row-fluid"><!-- BEGIN PAGE CONTENT-->
	<div class="col-md-12">
		<table class="table table-striped table-bordered table-hover datatable">
		     <thead>
		        <tr>
		            <th>编号</th>
		            <th>名称</th>
		            <th>创建人</th>
		            <th>创建日期</th>
		        </tr>
		     </thead>
		     <tbody>
		     	{%for item in evaluateSchool%}
					<tr>
		                <td>{{item.id}}</td>
		                <td>{{item.name}}</td>
		                <td>{{item.create_user_admin}}</td>
		                <td>{{item.insertTime|date('Y-m-d H:i:s')}}</td>
		                <td>
		                    <a type="button" class="btn red btn-xs ajaxify"  href="{{ path('evaluation_evaluate_school_delete',{'id':item.id})}}">删除</a>
		                    <a type="button" class="btn green btn-xs" href="{{ path('evaluation_evaluate_school_update',{'id':item.id})}}">编辑</a>
		                </td>
		            </tr>
				{%endfor%}
		     </tbody>
		</table>
	</div>
</div><!-- END PAGE CONTENT-->
